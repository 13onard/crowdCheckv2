<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".adminActivity">

    <View
        android:id="@+id/adminView"
        android:layout_width="413dp"
        android:layout_height="100dp"
        android:foreground="@color/blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/adminTxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/interreg"
        android:text="ADMIN / STAFF"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/adminView"
        app:layout_constraintEnd_toEndOf="@+id/adminView"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/adminView"
        app:layout_constraintTop_toTopOf="@+id/adminView"
        app:layout_constraintVertical_bias="0.37" />

    <TextView
        android:id="@+id/adminTxt2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/interreg"
        android:text="DASHBOARD"
        android:textColor="@color/white"
        android:textSize="18dp"
        app:layout_constraintBottom_toBottomOf="@+id/adminView"
        app:layout_constraintEnd_toEndOf="@+id/adminView"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/adminView"
        app:layout_constraintTop_toTopOf="@+id/adminTxt"
        app:layout_constraintVertical_bias="0.484" />

    <ImageButton
        android:id="@+id/adminBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/adminView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icons8_back_30" />

    <View
        android:id="@+id/notifView"
        android:layout_width="413dp"
        android:layout_height="100dp"
        android:foreground="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/adminView"
        app:layout_constraintVertical_bias="0.25" />

    <ImageView
        android:id="@+id/setNotif"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/notifView"
        app:layout_constraintEnd_toEndOf="@+id/notifView"
        app:layout_constraintHorizontal_bias="0.16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/notifView"
        app:srcCompat="@drawable/icons8_add_reminder_96"/>

    <TextView
        android:id="@+id/setNotiftxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Set Notifications"
        app:layout_constraintBottom_toBottomOf="@+id/notifView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/setNotif"
        app:layout_constraintTop_toTopOf="@+id/notifView"
        android:textSize="25dp"
        android:fontFamily="@font/interbold"
        android:textColor="@color/blue"/>

    <View
        android:id="@+id/checkFeedbackview"
        android:layout_width="413dp"
        android:layout_height="100dp"
        android:foreground="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/notifView"
        app:layout_constraintVertical_bias="0.07999998" />

    <ImageView
        android:id="@+id/checkFeedback"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkFeedbackview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/checkFeedbackview"
        app:srcCompat="@drawable/icons8_open_email_96" />

    <TextView
        android:id="@+id/checkFeedbacktxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginEnd="69dp"
        android:fontFamily="@font/interbold"
        android:text="Check Feedback"
        android:textColor="@color/blue"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/checkFeedbackview"
        app:layout_constraintEnd_toEndOf="@+id/checkFeedbackview"
        app:layout_constraintHorizontal_bias="0.48"
        app:layout_constraintStart_toEndOf="@+id/checkFeedback"
        app:layout_constraintTop_toTopOf="@+id/checkFeedbackview" />

    <View
        android:id="@+id/logHistoryview"
        android:layout_width="413dp"
        android:layout_height="100dp"
        android:foreground="@color/gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/checkFeedbackview"
        app:layout_constraintVertical_bias="0.127" />

    <ImageView
        android:id="@+id/checkLog"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toBottomOf="@+id/logHistoryview"
        app:layout_constraintEnd_toEndOf="@+id/logHistoryview"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/logHistoryview"
        app:srcCompat="@drawable/icons8_log_96" />

    <TextView
        android:id="@+id/logHistorytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/interbold"
        android:text="Log History"
        android:textColor="@color/blue"
        android:textSize="25dp"
        app:layout_constraintBottom_toBottomOf="@+id/logHistoryview"
        app:layout_constraintEnd_toEndOf="@+id/logHistoryview"
        app:layout_constraintHorizontal_bias="0.31"
        app:layout_constraintStart_toEndOf="@+id/checkLog"
        app:layout_constraintTop_toTopOf="@+id/logHistoryview" />

</androidx.constraintlayout.widget.ConstraintLayout>